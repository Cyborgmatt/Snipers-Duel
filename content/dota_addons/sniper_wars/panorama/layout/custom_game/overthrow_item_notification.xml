<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/overthrow_item_notification.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/overthrow_notification.js" />
	</scripts>
	<Panel hittest="false" class="OverthrowNotification">
		<Panel hittest="false" class="OverthrowItemNotification">
			<Panel hittest="false" id="AlertTimer" text="">
				<Label hittest="false" id="AlertTimer_Text" class="NotificationMessage" text="" />
				<Label hittest="false" id="AlertTimer_Remaining" class="NotificationMessage" text="#Remaining" />
			</Panel>
			<Panel hittest="false" id="Overtime" text="">
				<Label hittest="false" id="Overtime_Text" class="NotificationMessage" text="#Overtime" />
				<Label hittest="false" id="Overtime_Instructions" class="NotificationMessage" text="#OvertimeInstructions" />
				<Label hittest="false" id="Overtime_Goal" class="NotificationMessage" text="" />
			</Panel>
			<Panel hittest="false" id="AlertMessage" text="">
				<Label hittest="false" id="AlertMessage_Chest" class="NotificationMessage" text="" html="true" />
				<Label hittest="false" id="AlertMessage_Delivery" class="NotificationMessage" text="" html="true" />
			</Panel>
			<Panel hittest="false" id="PickupMessage" text="">
				<Panel hittest="false" id="PickupImages" text="">
					<Image hittest="false" id="PickupMessage_Hero" />
					<Image hittest="false" id="PickupMessage_Chest" />
					<Image hittest="false" id="PickupMessage_Item" />
					<Image src="file://{resources}/images/items/ambient_sorcery.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/bear_cloak.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/bogduggs_baldric.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/bogduggs_lucky_femur.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/craggy_coat.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/Creed_of_Omniscience.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/gravel_foot.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/guardian_shell.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/longclaws_amulet.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/pelt_of_the_old_wolf.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/precious_egg.png" id="PickupMessage_Item"/>
					<Image src="file://{resources}/images/items/slippers_of_the_abyss.png" id="PickupMessage_Item"/>
				</Panel>
				<Label hittest="false" id="PickupMessage_Hero_Text" class="NotificationMessage" text="#OverthrowHeroPickup" />
				<Label hittest="false" id="PickupMessage_Text" class="NotificationMessage" text="#OverthrowTextPickup" />
				<Label hittest="false" id="PickupMessage_Item_Text" class="NotificationMessage" text="#OverthrowItemPickup" />
			</Panel>
			<Panel hittest="false" id="KillMessage" text="">
			</Panel>
		</Panel>
	</Panel>
</root>
